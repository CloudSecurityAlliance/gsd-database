      "work_around": [
        {
          "lang": "en",
          "supportingMedia": [
            {
              "base64": false,
              "type": "text/html",
              "value": "<p>This issue can be avoided by disabling CFM MIP functionality.</p> <tt>[ protocols oam ethernet connectivity-fault-management maintenance-domain mip-half-function none ]</tt>"
            }
          ],
          "value": "This issue can be avoided by disabling CFM MIP functionality.\n\n [ protocols oam ethernet connectivity-fault-management maintenance-domain mip-half-function none ]"
