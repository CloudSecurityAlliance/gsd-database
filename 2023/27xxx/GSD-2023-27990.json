                "problemtype_data": [
                    {
                        "description": [
                            {
                                "lang": "eng",
                                "value": "CWE-79: Improper Neutralization of Input During Web Page Generation ('Cross-site Scripting')"
                            }
                        ]
                    }
                ]
            },
