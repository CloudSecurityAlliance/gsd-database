            "solution": [
                {
                    "lang": "en",
                    "value": "If using tcpdump 4.99.3, upgrade to 4.99.4."
                }
            ],
            "work_around": [
                {
                    "lang": "en",
                    "value": "Do not enable the SMB printer when building tcpdump 4.99.3."
                }
