                    "description_data": [
                        {
                            "lang": "en",
                            "value": "A flaw was found in the keystone catalog URL replacement. A user with permissions to register an endpoint could use this flaw to leak configuration data, including the master admin_token. Only keystone setups that allow non-cloud-admin users to create endpoints were affected by this issue."
                        }
                    ]
