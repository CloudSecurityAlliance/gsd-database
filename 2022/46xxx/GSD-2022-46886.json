                "description_data": [
                    {
                        "lang": "eng",
                        "value": "There exists an open redirect within the response list update functionality of ServiceNow. This allows attackers to redirect users to arbitrary domains when clicking on a URL within a service-now domain."
                    }
                ]
            },
