                "problemtype_data": [
                    {
                        "description": [
                            {
                                "lang": "eng",
                                "value": "Successful execution of this attack could result in the ability to perform actions within the context of another user including reading, updating, inserting, or deleting data accessible to TIBCO BusinessConnect."
